{
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "npm run build",
      "env": {
        "NODE_VERSION": "20.19.0"
      }
    },
    "deploy": {
      "startCommand": "npm run start",
      "healthcheckPath": "/",
      "healthcheckTimeout": 100,
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }
